  entity Photo{
    name : String
  }
  
  trait HasPhoto{}
  
  relation HasPhoto.[[photo]] 1 <-> Photo
  
  entity Page extends HasPhoto {
    photoName : String = [[photo]].name
  }
]] resolve #2 to #1

test resolve relation attribute subtype [[
  entity Photo{
    [[name]] : String
  }
  
  trait HasPhoto{}
  
  relation HasPhoto 1 <-> Photo
  
  entity Page extends HasPhoto {
    photoName : String = photo.[[name]]
